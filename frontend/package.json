{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite --open",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "type-gen": "node utils/supabase-types.js",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && eslint ./src -c custom.eslintrc.cjs --ext .ts --fix --ignore-path .gitignore",
    "lint2": "eslint ./src -c custom.eslintrc.cjs --ext .ts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.2",
    "@supabase/supabase-js": "^1.35.6",
    "@vueuse/core": "^9.1.0",
    "ky": "^0.31.1",
    "oxide.ts": "^1.0.5",
    "pinia": "^2.0.16",
    "pinia-shared-state": "^0.2.8",
    "unplugin-auto-import": "^0.10.1",
    "vant": "^4.0.0-alpha.4",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.45",
    "@unocss/preset-wind": "^0.45.8",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-plugin-result": "file:./eslint-plugin-result",
    "eslint-plugin-stefnotch": "file:../eslint-plugin-stefnotch",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "unocss": "^0.45.6",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.8",
    "vue-tsc": "^0.40.1"
  }
}
